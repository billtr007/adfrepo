{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory Name",
			"defaultValue": "bt-DataFactoryV2"
		},
		"AzureBlobTarget1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobTarget1'"
		},
		"Sql_OnMyLaptop_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Sql_OnMyLaptop'"
		},
		"Sql_OnMyLaptop_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'Sql_OnMyLaptop'"
		},
		"Sql_OnMyLaptop_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "DESKTOP-HIDI3VC\\Bill"
		},
		"Teradata_onprem1_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "TDIDWCOP1.unix.gsm1900.org"
		},
		"Teradata_onprem1_properties_typeProperties_username": {
			"type": "string",
			"defaultValue": "APPL_SSI_AZURE"
		},
		"razr_keyvault_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://bt-keyvault01.vault.azure.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas Token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobTarget1_connectionString": {
						"value": "[parameters('AzureBlobTarget1_connectionString')]"
					},
					"Sql_OnMyLaptop_connectionString": {
						"value": "[parameters('Sql_OnMyLaptop_connectionString')]"
					},
					"Sql_OnMyLaptop_password": {
						"value": "[parameters('Sql_OnMyLaptop_password')]"
					},
					"razr_keyvault_properties_typeProperties_baseUrl": {
						"value": "[parameters('razr_keyvault_properties_typeProperties_baseUrl')]"
					},
					"Sql_OnMyLaptop_properties_typeProperties_userName": {
						"value": "[parameters('Sql_OnMyLaptop_properties_typeProperties_userName')]"
					},
					"Teradata_onprem1_properties_typeProperties_server": {
						"value": "[parameters('Teradata_onprem1_properties_typeProperties_server')]"
					},
					"Teradata_onprem1_properties_typeProperties_username": {
						"value": "[parameters('Teradata_onprem1_properties_typeProperties_username')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}