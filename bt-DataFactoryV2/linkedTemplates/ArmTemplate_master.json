{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory Name",
			"defaultValue": "bt-DataFactoryV2"
		},
		"TDIDWCOP1_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "TDIDWCOP1.unix.gsm1900.org"
		},
		"TDIDWCOP1_properties_typeProperties_username": {
			"type": "string",
			"defaultValue": "APPL_SSI_AZURE"
		},
		"razr_keyvault_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://bt-keyvault01.vault.azure.net/"
		},
		"LS_FileShare_Temp_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "fileuser01"
		},
		"LS_Web_Generic_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://www2.census.gov/programs-surveys/acs/summary_file/2016/data/5_year_by_state/"
		},
		"LS_Blob_Destination1_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "BlobDestination1"
		},
		"LS_SQL_Desktop_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "DESKTOPHIDI3VC"
		},
		"LS_SQL_Desktop_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "DESKTOP-HIDI3VC\\sqlsrv"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas Token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"razr_keyvault_properties_typeProperties_baseUrl": {
						"value": "[parameters('razr_keyvault_properties_typeProperties_baseUrl')]"
					},
					"LS_Web_Generic_properties_typeProperties_url": {
						"value": "[parameters('LS_Web_Generic_properties_typeProperties_url')]"
					},
					"TDIDWCOP1_properties_typeProperties_server": {
						"value": "[parameters('TDIDWCOP1_properties_typeProperties_server')]"
					},
					"TDIDWCOP1_properties_typeProperties_username": {
						"value": "[parameters('TDIDWCOP1_properties_typeProperties_username')]"
					},
					"LS_FileShare_Temp_properties_typeProperties_userId": {
						"value": "[parameters('LS_FileShare_Temp_properties_typeProperties_userId')]"
					},
					"LS_Blob_Destination1_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('LS_Blob_Destination1_properties_typeProperties_connectionString_secretName')]"
					},
					"LS_SQL_Desktop_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('LS_SQL_Desktop_properties_typeProperties_connectionString_secretName')]"
					},
					"LS_SQL_Desktop_properties_typeProperties_userName": {
						"value": "[parameters('LS_SQL_Desktop_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}