{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory Name",
			"defaultValue": "bt-DataFactoryV2"
		},
		"AzureBlobTarget1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobTarget1'"
		},
		"Sql_OnMyLaptop_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Sql_OnMyLaptop'"
		},
		"Sql_OnMyLaptop_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'Sql_OnMyLaptop'"
		},
		"Sql_OnMyLaptop_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "DESKTOP-HIDI3VC\\Bill"
		},
		"Teradata_onprem1_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "TDIDWCOP1.unix.gsm1900.org"
		},
		"Teradata_onprem1_properties_typeProperties_username": {
			"type": "string",
			"defaultValue": "APPL_SSI_AZURE"
		},
		"razr_keyvault_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://bt-keyvault01.vault.azure.net/"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/AzureBlobTarget1')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureBlobStorage",
				"typeProperties": {
					"connectionString": "[parameters('AzureBlobTarget1_connectionString')]"
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/Sql_OnMyLaptop')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "SqlServer",
				"typeProperties": {
					"connectionString": "[parameters('Sql_OnMyLaptop_connectionString')]",
					"userName": "[parameters('Sql_OnMyLaptop_properties_typeProperties_userName')]",
					"password": {
						"type": "SecureString",
						"value": "[parameters('Sql_OnMyLaptop_password')]"
					}
				},
				"connectVia": {
					"referenceName": "IR-VM01",
					"type": "IntegrationRuntimeReference"
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/integrationRuntimes/IR-VM01')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/Teradata_onprem1')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "Teradata",
				"typeProperties": {
					"server": "[parameters('Teradata_onprem1_properties_typeProperties_server')]",
					"authenticationType": "Basic",
					"username": "[parameters('Teradata_onprem1_properties_typeProperties_username')]",
					"password": {
						"type": "AzureKeyVaultSecret",
						"store": {
							"referenceName": "razr_keyvault",
							"type": "LinkedServiceReference"
						},
						"secretName": "onp-tdidw-applssiazure-password"
					}
				},
				"connectVia": {
					"referenceName": "IR-VM01",
					"type": "IntegrationRuntimeReference"
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/integrationRuntimes/IR-VM01')]",
				"[concat(variables('factoryId'), '/linkedServices/razr_keyvault')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/razr_keyvault')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureKeyVault",
				"typeProperties": {
					"baseUrl": "[parameters('razr_keyvault_properties_typeProperties_baseUrl')]"
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/IR-VM01')]",
			"type": "Microsoft.DataFactory/factories/integrationRuntimes",
			"apiVersion": "2018-06-01",
			"properties": {
				"type": "SelfHosted",
				"typeProperties": {}
			},
			"dependsOn": []
		}
	]
}