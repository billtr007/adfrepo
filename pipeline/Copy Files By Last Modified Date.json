{
	"name": "Copy Files By Last Modified Date",
	"properties": {
		"description": "Copy new and changed files only by using LastModifiedDate",
		"activities": [
			{
				"name": "CopyNewFiles",
				"description": "Copy new and changed files only by using LastModifiedDate",
				"type": "Copy",
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"typeProperties": {
					"source": {
						"type": "FileSystemSource",
						"recursive": true
					},
					"sink": {
						"type": "BlobSink"
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "DS_FileShareName",
						"type": "DatasetReference",
						"parameters": {
							"SubFolderName": {
								"value": "@pipeline().parameters.FileShare_Source",
								"type": "Expression"
							}
						}
					}
				],
				"outputs": [
					{
						"referenceName": "SK1_Blob_Destination1",
						"type": "DatasetReference",
						"parameters": {
							"ContainerName": {
								"value": "@pipeline().parameters.Blob_Destination",
								"type": "Expression"
							}
						}
					}
				]
			}
		],
		"parameters": {
			"FileShare_Source": {
				"type": "String",
				"defaultValue": "*"
			},
			"Blob_Destination": {
				"type": "String",
				"defaultValue": "store1"
			},
			"LastModified_From": {
				"type": "String",
				"defaultValue": "2019-02-01"
			},
			"LastModified_To": {
				"type": "String",
				"defaultValue": "2019-05-11"
			}
		}
	}
}